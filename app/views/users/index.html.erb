<% provide :title, t(".all_user") %>
<div class="container">
  <div class="row">
    <div class="col-md-9">
      <ul class="row users">
        <%= render @users %>
        <%= will_paginate @users %>
      </ul>
    </div>
    <div class="col-md-3">
      <div class="tech-btm most-like-posts">
        <h4><%= link_to t(".most_like_post"), posts_path(most_like: "") %></h4>
        <%= render partial: "posts/sidebar_post", collection: @sidebar_posts %>
      </div>
    </div>
  </div>
</div>
